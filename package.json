{
    "private": true,
    "scripts": {
        "start": "parcel serve --no-autoinstall frontend/index.html --dist-dir=public --port=80",
        "build": "yarn run clean && parcel build --no-autoinstall frontend/index.html --dist-dir=public --no-source-maps",
        "build-production": "[ \"$BASE_URL\" != '$BASE_URL' ] && yarn run build --public-url=$BASE_URL",
        "clean": "rimraf --glob 'public/*' 'public/.*'",
        "test": "jest",
        "test:types": "tsc",
        "test:prettier": "prettier --check 'frontend/**/*.{ts,tsx}'"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.11.0",
        "@types/jest": "^29.2.0",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "jest": "^29.2.1",
        "parcel": "^2.11.0",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "prettier": "^2.7.1",
        "rimraf": "^5.0.5",
        "sass": "^1.71.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.3.0-beta2",
        "@fortawesome/free-solid-svg-icons": "^6.0.0-beta2",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@popperjs/core": "^2.4.4",
        "@tanstack/react-virtual": "^3.0.0-alpha.0",
        "classnames": "^2.3.2",
        "ogv": "^1.8.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-popper": "^2.2.3"
    },
    "packageManager": "yarn@4.1.0",
    "staticFiles": [
        {
            "staticPath": "frontend/static"
        },
        {
            "staticPath": "node_modules/ogv/dist",
            "staticOutPath": "ogv"
        }
    ],
    "alias": {
        "process": false
    },
    "browserslist": [
        "last 1 version"
    ]
}
